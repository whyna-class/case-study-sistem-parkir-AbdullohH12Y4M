generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

enum jenisKendaraan {
  RODA2
  RODA4
}

model Parkir {
  id               Int            @id @default(autoincrement())
  platNomor        String
  jenisKendaraan   jenisKendaraan
  durasi           Int
  total            Int
  createdAt        DateTime       @default(now())
  updatedAt        DateTime       @updatedAt
}